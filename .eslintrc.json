{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "globals": {
    "chrome": true
  },
  "rules": {
    "max-len": "off",
    "class-methods-use-this": "off",
    "no-else-return":"off",
    "react/sort-comp": "off",
    "jsx-a11y/accessible-emoji": "off",
    "brace-style": "off",
    "import/extensions": "off"
  },
  "parser": "babel-eslint"
}